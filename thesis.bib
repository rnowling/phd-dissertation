title = {{I-TASSER: a unified platform for automated protein structure and function prediction.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2849174\&tool=pmcentrez\&rendertype=abstract},
volume = {5},
year = {2010}
}
