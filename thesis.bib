keywords = {Animals,Chromosome Mapping,Chromosome Mapping: methods,Databases, Protein,Evolution, Molecular,Expressed Sequence Tags,Genome, Human,Genome, Human: genetics,Humans,Mice,Phylogeny,Receptors, G-Protein-Coupled,Receptors, G-Protein-Coupled: genetics,Sequence Alignment,Sequence Alignment: methods,Sequence Analysis, Protein,Sequence Analysis, Protein: methods},
